class Vehicle:
    def __init__(self, brand, model, year):
        self.brand = brand  
        self.model = model  
        self.year = year   

    def display_info(self):
        """Prints vehicle details."""
        print(f"Vehicle: {self.year} {self.brand} {self.model}")


car = Vehicle("Toyota", "Corolla", 2022)
car.display_info()  
