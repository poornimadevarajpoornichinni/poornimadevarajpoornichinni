class Car:
    def __init__(self, brand, model):
        self.brand = brand
        self.model = model

    def display_info(self):
        print(f"Car Brand: {self.brand}")
        print(f"Car Model: {self.model}")

# Creating an object of the Car class
my_car = Car("Toyota", "Corolla")

# Displaying the car's information
my_car.display_info()
